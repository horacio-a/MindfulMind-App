{
  "expo": {
    "owner": "horacio.dev",
    "name": "MindfulMind-app",
    "slug": "mindfulmind-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "notification": {
      "icon": "./assets/s-white.png"
    },
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/s-white.png",
          "color": "#ffffff"
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "sdkVersion"
      }
    },
    "android": {
      "package": "com.apk.mindfulmind",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "49a4d4a0-80f7-4e53-ae42-d34494b9c746"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/49a4d4a0-80f7-4e53-ae42-d34494b9c746"
    },
    "androidStatusBar": {
      "backgroundColor": "#1e1e1e",
      "translucent": false
    }
  }
}